# Frontend Environment Variables

# Backend API URL
REACT_APP_BACKEND_URL=http://localhost:8000

# Google Gemini API Key for AI features
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# PayPal Client ID for payment integration
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id_here

# Optional: Disable source maps in production
GENERATE_SOURCEMAP=false

# Note: In CI/CD, dummy values can be used if these features are not critical for the build